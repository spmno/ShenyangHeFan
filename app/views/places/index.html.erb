<h1><%= I18n.t"list_place" %></h1>

<div class="row">
  <div class="col-md-2"><%= I18n.t"name" %></div>
  <div class="col-md-4"><%= I18n.t"district" %></div>
  <div class="col-md-2">""</div>
  <div class="col-md-2">""</div>
  <div class="col-md-2">""</div>
</div>



<% @places.each do |place| %>
  <div class="row">
    <div class="col-md-2"><%= place.name %></div>
    <div class="col-md-4"><%= place.district_id %></div>
    <div class="col-md-2"><%= link_to 'Show', place %></td>
    <div class="col-md-2"><%= link_to 'Edit', edit_place_path(place) %></div>
    <div class="col-md-2"><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></div>
  </div>
<% end %>


<br>

